<% if has_entry_menu? %>
	<h2 class="font-bold text-lg text-white px-8 py-4 bg-fus-blue sticky-header">Entry</h2>
	<div class="mt-4 px-4">
		<h3 class="font-semibold text-white">Asset</h3>
		<ul>
			<li>
					<%= link_to "#{t('activerecord.models.capital_proposal')}",
					admin_capital_proposals_path,
					class: "px-2 text-white hover:underline",
					data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>

			<li>
					<%= link_to "#{t('activerecord.models.request_for_purchase')}",
					admin_request_for_purchases_path,
					class: "px-2 text-white hover:underline",
					data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>

			<li>
					<%= link_to "#{t('activerecord.models.purchase_order')}",
					admin_purchase_orders_path,
					class: "px-2 text-white hover:underline",
					data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>

			<li>
					<%= link_to "#{t('activerecord.models.delivery_order')}",
					admin_delivery_orders_path,
					class: "px-2 text-white hover:underline",
					data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>

			<li>
					<%= link_to "Register #{t('activerecord.models.asset').downcase}",
					admin_assets_path,
					class: "px-2 text-white hover:underline",
					data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>
			<li><a href="#" class="px-2 text-white hover:underline">Update asset</a></li>
			<li>
					<%= link_to "#{t('custom.label.register')} #{t('activerecord.models.user_asset')}",
					admin_user_assets_path,
					class: "px-2 text-white hover:underline",
					data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>
			<li><a href="#" class="px-2 text-white hover:underline">Close berita acara</a></li>
			<li><a href="#" class="px-2 text-white hover:underline">Change tagging id</a></li>
		</ul>
	</div>

	<div class="mt-4 px-4">
			<h3 class="font-semibold text-white">Asset and Component</h3>
			<ul>
			<li>
					<%= link_to t("activerecord.models.asset_type"), admin_asset_types_path, class: "px-2 text-white hover:underline", data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>

			<li>
					<%= link_to t("activerecord.models.asset_item_type"), admin_asset_item_types_path, class: "px-2 text-white hover:underline", data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>

			<li>
					<%= link_to t("activerecord.models.asset_model"), admin_asset_models_path, class: "px-2 text-white hover:underline", data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>

			<li>
					<%= link_to t("activerecord.models.component_type"), admin_component_types_path, class: "px-2 text-white hover:underline", data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>
			<li>
					<%= link_to t("activerecord.models.component"), admin_components_path, class: "px-2 text-white hover:underline", data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>
			<li>
					<%= link_to t("activerecord.models.sofware"), admin_softwares_path, class: "px-2 text-white hover:underline", data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>
			</ul>
	</div>

	<div class="mt-4 px-4">
			<h3 class="font-semibold text-white">Location</h3>
			<ul>
			<li>
					<%= link_to t("activerecord.models.project"), admin_projects_path, class: "px-2 text-white hover:underline", data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>
			<li>
					<%= link_to t("activerecord.models.site_group"), admin_site_groups_path, class: "px-2 text-white hover:underline", data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>
			<li>
					<%= link_to t("activerecord.models.site"), admin_sites_path, class: "px-2 text-white hover:underline", data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>
			<li>
					<%= link_to t("activerecord.models.department"), admin_departments_path, class: "px-2 text-white hover:underline", data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>

			<li>
					<%= link_to t("activerecord.models.site_default"), admin_site_defaults_path, class: "px-2 text-white hover:underline", data: { turbo_frame: "main-content", turbo_action: "advance"} %>
			</li>
			</ul>
	</div>

	<% menu_masters_path = [] %>
	<% if menu_masters.present? %>
			<% menu_masters.each do |menu_master| %>
			<% menu_masters_path << menu_master.route_paths %>        
			<% end %>

			<% flat_menus = menu_masters_path.flatten.sort %>

			<% if flat_menus.present? %>
			<div class="mt-4 px-4">
					<h3 class="font-semibold text-white">Master</h3>

					<ul>
					<% flat_menus.each do |menu| %>
							<li><%= link_to t(menu.label), 
									send(menu.path),
									class: "px-2 text-white hover:underline",
									data: { 
											turbo_frame: "main-content",
											turbo_action: "advance",
											turbo_prefetch: false
									}
									%>
							</li>
					<% end %>
					</ul>
					
			</div>
			<% end %>
	<% end %>
<% end %>